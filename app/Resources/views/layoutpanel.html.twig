            </li>
             <li>
              <a href="{{path('raa_user_verEditOrganizacion')}}"  >Creando Sonrisas</a>
            </li>
            
          </ul>
        </li>
        
        
        <li class="nav-item" data-toggle="tooltip" data-placement="right" title="Link">
          <a class="nav-link" href="{{path('raa_user_verConfiguracion')}}">
            <i class="fa fa-gears"></i>
            <span class="nav-link-text">Configuracion</span>
          </a>
        </li>
      </ul>
     
      <ul class="navbar-nav ml-auto">
       
       <!-- NOMBRE DEL USAURIO -->
       
        <li class="nav-item">
          <a class="nav-link" data-toggle="modal" data-target="#exampleModal">
            <i class="fa fa-fw fa-sign-out"></i>{{user1.nombre}} {{user1.apellido}} </a>
        </li>
      </ul>
